{
  "name": "redis-x",
  "version": "0.0.2",
  "main": "dist/index.js",
  "license": "MIT",
  "description": "a simple alternative to redis-cli",
  "author": {
    "name": "Alexander Acebo",
    "email": "aacebowork@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aacebo/redis-x.git"
  },
  "keywords": [
    "redis",
    "gui",
    "electron"
  ],
  "scripts": {
    "ng": "ng",
    "start": "npm run build && electron .",
    "build": "ng build --base-href ./ && npm run build:electron",
    "build:prod": "npm run clean && ng build --prod --base-href ./ && npm run build:electron",
    "build:clean": "npm run clean && ng build --base-href ./ && npm run build:electron",
    "build:electron": "tsc -p src/electron",
    "clean": "rimraf dist",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "publish": "rimraf pkg && npm run build:prod && electron-builder -ml --publish always",
    "pkg": "rimraf pkg && npm run build:prod && electron-builder",
    "pkg:all": "rimraf pkg && npm run build:prod && electron-builder -ml",
    "pkg:win": "rimraf pkg && npm run build:prod && electron-builder --win",
    "pkg:mac": "rimraf pkg && npm run build:prod && electron-builder --mac",
    "pkg:linux": "rimraf pkg && npm run build:prod && electron-builder --linux"
  },
  "build": {
    "appId": "Redis-X",
    "productName": "Redis-X",
    "files": [
      "dist"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "aacebo"
      }
    ],
    "directories": {
      "output": "pkg",
      "buildResources": "dist/assets"
    },
    "mac": {
      "icon": "logo/png/512x512.png",
      "category": "public.sockets"
    },
    "linux": {
      "icon": "logo/png/512x512.png",
      "category": "development.debugger",
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~10.0.9",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "~10.0.9",
    "@angular/compiler": "~10.0.9",
    "@angular/core": "~10.0.9",
    "@angular/forms": "~10.0.9",
    "@angular/localize": "~10.0.9",
    "@angular/platform-browser": "~10.0.9",
    "@angular/platform-browser-dynamic": "~10.0.9",
    "@angular/router": "~10.0.9",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "bootstrap": "^4.5.2",
    "chalk": "^4.1.0",
    "codemirror": "^5.57.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "electron-is-dev": "^1.2.0",
    "electron-updater": "^4.3.5",
    "feather-icons": "^4.28.0",
    "ngx-toastr": "^13.0.0",
    "redis": "^3.0.2",
    "rxjs": "~6.5.5",
    "sequelize": "^6.3.5",
    "sqlite3": "^5.0.0",
    "tslib": "^2.0.0",
    "uuid": "^8.3.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.6",
    "@angular/cli": "~10.0.6",
    "@angular/compiler-cli": "~10.0.9",
    "@types/codemirror": "^0.0.97",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/redis": "^2.8.27",
    "@types/sequelize": "^4.28.9",
    "@types/uuid": "^8.3.0",
    "codelyzer": "^6.0.0",
    "electron": "^10.1.0",
    "electron-builder": "^22.8.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
